<map proto="1.3.3">
<name>Access</name>
<version>1.0.0</version>
<objective>
Capture the Red and Pink wool or eliminate the other team.
</objective>
<authors>
	<author>_IQ</author>
	<author>salalu</author>
</authors>
<contributors>
	<contributor contribution="Feedback and Map XML">Aceman1998</contributor>
</contributors>
<include src="tutorial.xml"/>
<tutorial>
</tutorial>
<teams>
	<team color="blue" max="14">Blue Team</team>
	<team color="dark green" max="14">Green Team</team>
</teams>
<spawns>
	<spawn team="green" kit="green" yaw="135">
		<cylinder base="42,7,41" radius="2" height="0"/>
	</spawn>
	<spawn team="blue" kit="blue" yaw="-45">
		<cylinder base="1,7,0" radius="2" height="0"/>
	</spawn>
	<default yaw="90">
	<cylinder base="21,35,20" radius="2" height="0"/>
</default>
</spawns>
<kits>
	<kit name="spawn">
		<item slot="0">stone sword</item>
		<item slot="1">bow</item>
		<item slot="2">golden apple</item>
		<item slot="3" amount="16">bread</item>
		<item slot="8" amount="16">arrow</item>
		<potion duration="4" amplifier="4">damage_resistance</potion>
	</kit>
	<kit name="green" parents="spawn">
		<helmet color="667F33">leather helmet</helmet>
		<chestplate color="667F33">leather chestplate</chestplate>
		<leggings color="667F33">leather leggings</leggings>
		<boots color="667F33">leather boots</boots>
	</kit>
	<kit name="blue" parents="spawn">
		<helmet color="334CB2">leather helmet</helmet>
		<chestplate color="334CB2">leather chestplate</chestplate>
		<leggings color="334CB2">leather leggings</leggings>
		<boots color="334CB2">leather boots</boots>
	</kit>
</kits>
<toolrepair>
	<tool>stone sword</tool>
	<tool>bow</tool>
</toolrepair>
<itemremove>
	<item>stone sword</item>
	<item>arrow</item>
	<item>leather helmet</item>
	<item>leather chestplate</item>
	<item>leather leggings</item>
	<item>leather boots</item>
	<item>golden apple</item>
	<item>wool:14</item>
	<item>wool:6</item>
</itemremove>
<killreward>
	<item amount="4">arrow</item>
</killreward>
<regions>
	<apply block="deny-all">
	<negative>
		<rectangle min="0,0" max="0,0"/>
	</negative>
	</apply>
</regions>
<wools>
	<wool team="blue" color="red">
		<block location="6.5,5,0.5"/>
	</wool>
	<wool team="blue" color="pink">
		<block location="1.5,5,5.5"/>
	</wool>
	<wool team="green" color="red">
		<block location="37.5,5,41.5"/>
	</wool>
	<wool team="green" color="pink">
		<block location="42.5,5,36.5"/>
	</wool>
</wools>
</map>